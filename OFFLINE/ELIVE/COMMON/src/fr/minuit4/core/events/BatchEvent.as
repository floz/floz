/*** Written by :* @author Floz* www.floz.fr || www.minuit4.fr*/package fr.minuit4.core.events{	import fr.minuit4.core.commands.ICommand;	import flash.events.Event;	public class BatchEvent extends Event	{		private static const CLASS_ID:String = "batchevent";				public static var COMMAND_START:String = CLASS_ID + "batchevent_command_start";		public static var COMMAND_COMPLETE:String = CLASS_ID + "batchevent_command_complete";				public var command:ICommand;				public function BatchEvent( type:String, bubbles:Boolean = false, cancelable:Boolean = false )		{			super( type, bubbles, cancelable );		}				override public function clone():Event		{			return new BatchEvent( type, bubbles, cancelable );		}				override public function toString():String		{			return formatToString( "BatchEvent", "type", "bubbles", "cancelable" );		}	}}