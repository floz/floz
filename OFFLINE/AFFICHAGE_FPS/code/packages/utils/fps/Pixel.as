package utils.fps 
{
	
	public class Pixel 
	{
		public var sx:Number;
		public var sy:Number;
		public var vx:Number;
		public var vy:Number;
		
		public function Pixel( sx:Number = 0, sy:Number = 0 ) 
		{
			this.sx = sx;
			this.sy = sy;
			this.vx = sx;
			this.vy = sy;
		}
		
	}
	
}