/*** Written by :* @author Floz* www.floz.fr || www.minuit4.fr*/package fr.minuit4.core.datas.dynamics{	public class DynamicXML extends DynamicDatas	{			// - PRIVATE VARIABLES -----------------------------------------------------------				private var _datas:XML;				// - PUBLIC VARIABLES ------------------------------------------------------------				// - CONSTRUCTOR -----------------------------------------------------------------				public function DynamicXML( datas:XML )		{			this._datas = datas;						parseDatas();		}				// - EVENTS HANDLERS -------------------------------------------------------------				// - PRIVATE METHODS -------------------------------------------------------------				override protected function parseDatas():void		{			var list:XMLList = _datas.children();			for each( var propNode:XML in list )				_properties[ propNode.name() ] = propNode;		}				// - PUBLIC METHODS --------------------------------------------------------------				// - GETTERS & SETTERS -----------------------------------------------------------		}	}