/*** Written by :* @author Floz* www.floz.fr || www.minuit4.fr*/package fr.minuit4.tools.musicPlayer.core.views.device{	import fr.minuit4.tools.musicPlayer.core.views.ButtonComponent;	import flash.events.MouseEvent;	public class AMuteButton extends ButtonComponent	{			// - PRIVATE VARIABLES -----------------------------------------------------------				// - PUBLIC VARIABLES ------------------------------------------------------------				// - CONSTRUCTOR -----------------------------------------------------------------				public function AMuteButton()		{			super();		}				// - EVENTS HANDLERS -------------------------------------------------------------				override protected function onClick( e:MouseEvent ):void		{			if( !_musicManager.isMute() )				_musicManager.mute();		}				// - PRIVATE METHODS -------------------------------------------------------------				// - PUBLIC METHODS --------------------------------------------------------------				// - GETTERS & SETTERS -----------------------------------------------------------		}	}