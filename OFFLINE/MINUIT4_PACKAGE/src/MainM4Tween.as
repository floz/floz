/*** Written by :* @author Floz* www.floz.fr || www.minuit4.fr*/package {	import aze.motion.Eaze;	import fr.minuit4.utils.debug.FPS;	import flash.display.Graphics;	import flash.display.Sprite;	import flash.display.StageAlign;	import flash.display.StageScaleMode;	import flash.events.TimerEvent;	import flash.utils.Timer;	public class MainM4Tween extends Sprite	{			// - PRIVATE VARIABLES -----------------------------------------------------------				private var aSprites:/*Sprite*/Array;				// - PUBLIC VARIABLES ------------------------------------------------------------				// - CONSTRUCTOR -----------------------------------------------------------------				public function MainM4Tween()		{			stage.scaleMode = StageScaleMode.NO_SCALE;			stage.align = StageAlign.TOP_LEFT;						aSprites = [];			var s:Sprite;			var g:Graphics;			var i:int, n:int = 2000;			for( ; i<n; ++i )			{				s = new Sprite();				g = s.graphics;				g.beginFill( 0x000000, Math.random() * .6 + .4 );				g.drawRect( 0, 0, Math.random() * 30 + 10, Math.random() * 30 + 10 );				g.endFill();				s.x = stage.stageWidth * .5 - s.width * .5;				s.y = stage.stageHeight * .5 - s.height * .5;				aSprites.push( addChild( s ) );			}						addChild( new FPS() );						var timer:Timer = new Timer( 500 );			timer.addEventListener( TimerEvent.TIMER, onTimer );			timer.start();		}		// - EVENTS HANDLERS -------------------------------------------------------------				private function onTimer( e:TimerEvent ):void		{			var i:int = aSprites.length;			while( --i > -1 )			{//				M4Tween.to( aSprites[ i ], .5, { x: Math.random() * stage.stageWidth, y: Math.random() * stage.stageHeight } );//				TweenLite.to( aSprites[ i ], .5, { x: Math.random() * stage.stageWidth, y: Math.random() * stage.stageHeight } );				Eaze.to( aSprites[ i ], .5, { x: Math.random() * stage.stageWidth, y: Math.random() * stage.stageHeight } );			}		}				// - PRIVATE METHODS -------------------------------------------------------------				// - PUBLIC METHODS --------------------------------------------------------------				// - GETTERS & SETTERS -----------------------------------------------------------		}	}