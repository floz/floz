/*** Written by :* @author Floz* www.floz.fr || www.minuit4.fr*/package fr.minuit4.motion{	public class M4TweenProperty	{			// - PRIVATE VARIABLES -----------------------------------------------------------				// - PUBLIC VARIABLES ------------------------------------------------------------				public var name:String;		public var endValue:Number;		public var next:M4TweenProperty;				public var startValue:Number;		public var delta:Number;				// - CONSTRUCTOR -----------------------------------------------------------------				public function M4TweenProperty( name:String, endValue:Number, next:M4TweenProperty )		{			this.name = name;			this.endValue = endValue;			this.next = next;		}				// - EVENTS HANDLERS -------------------------------------------------------------				// - PRIVATE METHODS -------------------------------------------------------------				// - PUBLIC METHODS --------------------------------------------------------------				public function init( target:Object ):void		{			startValue = target[ name ];			delta = endValue - startValue;		}				public function dispose():void		{			if( next ) next.dispose();			next = null;		}				// - GETTERS & SETTERS -----------------------------------------------------------		}	}