/*** Written by :* @author Floz* www.floz.fr || www.minuit4.fr*/package com.wtf.mobiles{	import com.wtf.engines.renderer.IRenderable;	import com.wtf.misc.MovieBitmap;	public class Mobile extends MovieBitmap implements IRenderable	{			// - PRIVATE VARIABLES -----------------------------------------------------------				protected var _vx:Number = 0;		protected var _vy:Number = 0;				// - PUBLIC VARIABLES ------------------------------------------------------------				// - CONSTRUCTOR -----------------------------------------------------------------				public function Mobile()		{					}				// - EVENTS HANDLERS -------------------------------------------------------------				// - PRIVATE METHODS -------------------------------------------------------------				// - PUBLIC METHODS --------------------------------------------------------------				public function render( renderTime:Number ):void		{			this.x += _vx;// * renderTime * .001;			this.y += _vy;// * renderTime * .001; vy doit etre égal à 50 pour dire que le perso avance de 1px par frame.		}				// - GETTERS & SETTERS -----------------------------------------------------------		}	}