	<fx:Script>
		<![CDATA[
			protected function Button_click():void
			{
				var état:String = currentState;
				if ( état == 'close' ) {
					currentState='open';
				}
				if ( état == 'open' ) {
					currentState='close';
				}
			}
		]]>

	</fx:Script>
	<s:transitions>
		<s:Transition fromState="close" toState="open" autoReverse="true">
			<s:Parallel>
